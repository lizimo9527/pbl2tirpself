{
  "name": "tripself",
  "version": "1.0.0",
  "description": "旅行自助服务平台 - 基于uni-app开发",
  "main": "main.js",
  "scripts": {
    "dev": "uni",
    "dev:h5": "uni -p h5",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:app": "uni -p app",
    "build": "uni build",
    "build:h5": "uni build --platform h5",
    "build:mp-weixin": "uni build --platform mp-weixin",
    "build:app": "uni build --platform app",
    "serve": "vite preview"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0",
    "@dcloudio/uni-app-plus": "^3.0.0",
    "@dcloudio/uni-h5": "^3.0.0",
    "@dcloudio/uni-mp-weixin": "^3.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@dcloudio/uni-cli-shared": "^3.0.0",
    "@dcloudio/vite-plugin-uni": "^3.0.0",
    "vite": "^4.5.0",
    "sass": "^1.66.1"
  },
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ]
}